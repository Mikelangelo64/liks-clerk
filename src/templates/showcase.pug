section.section.showcase
  ._container
    .showcase__wrapper
      .showcase__header.section-header
        h2.section-header__title Our Work Showcase

      .showcase__main
        .showcase-slider.swiper.slider
          .swiper-wrapper
            each item in showcaseList
              .swiper-slide
                .showcase-slider__item
                  .showcase-slider-media
                    .showcase-slider-media__container
                      img(
                        src=require(`${item.media.path}`),
                        alt=`${item.title}`
                      )

                    .showcase-slider-media__hover
                      p.showcase-slider-media__title= item.media.hover.title

                      .showcase-slider-media__content
                        p.showcase-slider-media__percent= item.media.hover.percent

                        p.showcase-slider-media__subtitle= item.media.hover.subtitle

                  .showcase-slider__content
                    ul.showcase-slider-category
                      each category in item.categoryList
                        li.showcase-slider-category__item= category.title

                    p.showcase-slider__title= item.title

                    ul.showcase-slider-characteristics
                      each characteristic in item.characteristicList
                        li.showcase-slider-characteristics__item
                          p.showcase-slider-characteristics__title= characteristic.title

                          p.showcase-slider-characteristics__value= characteristic.value

        .showcase-slider-controls.slider-controls
          button.slider-prev.showcase-slider-prev
