footer.footer
  ._container
    .footer__wrapper
      .footer__main
        .footer__content
          a.custom-logo-link.footer__logo(href='#') 
            img.custom-logo(
              src=require('@/assets/img/logo-footer.svg'),
              alt='logo',
              draggable='false'
            )

          ul.footer-review.desktop
            each item in reviewFooterList
              li.footer-review__item
                .footer-review__info
                  .footer-review__subtitle Reviewed on

                  .footer-review__label
                    img(src=require(`${item.label}`), alt=`${item.alt}`)

                .footer-review-raiting
                  .footer-review-raiting-stars
                    - for(let i=0; i<5; i++)
                      .footer-review-raiting-stars__item(
                        class=`${i + 1 <= item.raiting ? 'filled' : 'empty'}`
                      ) 
                        if i + 1 <= item.raiting
                          img(
                            src=require('@/assets/img/review/star.svg'),
                            alt='star'
                          )
                        else
                          img(
                            src=require('@/assets/img/review/star-empty.svg'),
                            alt='star'
                          )

                  p.footer-review-raiting__description
                    strong= item.reviewCount
                    span reviews

        nav.footer-menu
          ul.footer-menu__list
            each item in footerMenu
              li.footer-menu__item
                p.footer-menu__title= item.title

                ul.footer-menu__sublist(
                  class=`${item.isSocial === 'true' ? 'socials' : ''}`
                )
                  each link in item.list
                    li
                      if item.isSocial && item.isSocial === 'true'
                        a.footer-menu-link(
                          href=`${link.href}`,
                          target=`${item.isTargetBlank === 'true' ? '_blank' : '_self'}`
                        ) 
                          span.footer-menu-link__label!= link.label

                          span.footer-menu-link__text= link.title

                      else
                        a.footer-menu-link(
                          href=`${link.href}`,
                          target=`${item.isTargetBlank === 'true' ? '_blank' : '_self'}`
                        ) 
                          span.footer-menu-link__text= link.title

      .footer__bottom
        ul.footer-review.phone
          each item in reviewFooterList
            li.footer-review__item
              .footer-review__info
                .footer-review__subtitle Reviewed on

                .footer-review__label
                  img(src=require(`${item.label}`), alt=`${item.alt}`)

              .footer-review-raiting
                .footer-review-raiting-stars
                  - for(let i=0; i<5; i++)
                    .footer-review-raiting-stars__item(
                      class=`${i + 1 <= item.raiting ? 'filled' : 'empty'}`
